<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>
  <display-name>Your favorite movie</display-name>
  <description>
    A servlet-based application that recommends genre based on a person's favorite movie.
  </description>
  <servlet>
    <servlet-name>RecommendMovie</servlet-name>
    <servlet-class>RecommendMovie</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RecommendMovie</servlet-name>
    <url-pattern>/RecommendMovie</url-pattern>
  </servlet-mapping>
</web-app>
